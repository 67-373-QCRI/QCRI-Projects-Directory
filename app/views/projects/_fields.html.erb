<%= f.input :name, label_html: { class: 'active' } %>
<%= f.input :description, label_html: { class: 'active' } %>
<%= f.input :members, as: :collection_select, collection: Researcher.all.to_a, include_blank: true, input_html: {multiple: true} %>
<%= f.association :team_leader, as: :collection_select, collection: Researcher.team_leaders.to_a, include_blank: false %>
<%= f.input :start_date, as: :date %>
<%= f.input :end_date, as: :date, include_blank: true %>
